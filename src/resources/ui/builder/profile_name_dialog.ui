<interface>
  <object class="AdwAlertDialog" id="dialog">
    <property name="default-response">add</property>
    <property name="close-response">cancel</property>
    <property name="focus-widget">entry</property>
    <property name="extra-child">
      <object class="AdwPreferencesGroup">
      <child>
        <object class="AdwEntryRow" id="entry">
        <property name="title">Profile Name</property>
        <property name="activates-default">true</property>
        </object>
      </child>
      </object>
    </property>
    <responses>
      <response id="cancel">_Cancel</response>
      <response id="add" appearance="suggested" enabled="false">_Add</response>
    </responses>
  </object>
</interface>
